    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "type": "module",
    "scripts": {
        "test": "vitest run",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
