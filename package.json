    "devDependencies": {
        "@types/jest": "^26.0.9",
        "jest": "^26.4.0",
        "prettier": "^2.0.5",
        "ts-jest": "^26.2.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.9.7"
    },
    "files": [
        "lib/**/*"
