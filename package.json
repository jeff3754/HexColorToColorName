    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.5",
        "globals": "^17.3.0",
        "prettier": "^3.8.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.54.0",
        "vitest": "^4.0.18"
